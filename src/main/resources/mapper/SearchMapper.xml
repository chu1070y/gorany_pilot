<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.gorany.mapper.SearchMapper">

	<select id="searchMenu" resultType="org.gorany.domain.MenuVO">
		select
		*
		from
		partner_store_menu
		where
		mname like concat("%",#{keyword},"%")
	</select>

	<select id="searchStore" resultType="org.gorany.domain.StoreVO">
		select
		*
		from
		partner_store
		where
		sname like concat("%",#{keyword},"%")
	</select>

	<select id="getMenuName" resultType="String">
		select concat('메뉴 - ',mname) from
		partner_store_menu
	</select>

	<select id="getStoreName" resultType="String">
		select concat('가게 - ',sname) from
		partner_store
	</select>
</mapper>
 
 